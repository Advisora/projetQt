      <property name="styleSheet">
       <string notr="true">/* --- QPushButton Styling (Bright Olive Green with Animation) --- */
QPushButton {
    background-color: #8B9B3D;       /* Bright Olive Green */
    color: white;                    /* White text */
    padding: 12px 30px;               /* Generous padding */
    font-size: 16px;                  /* Font size */
    font-weight: bold;                /* Bold text */
    border-radius: 8px;               /* Rounded corners */
    border: 2px solid #6C7C2F;        /* Dark olive border */
    min-width: 150px;                 /* Minimum button width */
    text-align: center;               /* Center text */
    transition: all 0.3s ease-in-out; /* Smooth transition */
    opacity: 0;                       /* Start with the text hidden */
    animation: fadeInText 0.5s forwards; /* Text fade-in animation */
}

/* --- Hover Effect for QPushButton (Bright Olive Green) --- */
QPushButton:hover {
    background-color: #A4C73C;        /* Lighter Olive Green on hover */
    box-shadow: 0px 5px 20px rgba(139, 155, 61, 0.6); /* Subtle shadow */
    cursor: pointer;                  /* Pointer cursor on hover */
    transform: scale(1.05);            /* Button slightly grows on hover */
}

/* --- Pressed State (Slight Movement and Darker Color) --- */
QPushButton:pressed {
    background-color: #7A8F33;        /* Darker Olive Green when pressed */
    box-shadow: none;                  /* No shadow on press */
    transform: scale(0.98);            /* Slightly shrinks when pressed */
}

/* --- Disabled State for QPushButton (Gray) --- */
QPushButton:disabled {
    background-color: #BDBDBD;         /* Gray background */
    color: #888888;                    /* Gray text */
    cursor: not-allowed;               /* Disable cursor */
}

/* --- Focused State for QPushButton (Glow Effect) --- */
QPushButton:focus {
    outline: none;                     /* Remove default outline */
    box-shadow: 0px 0px 10px rgba(139, 155, 61, 0.8); /* Glowing effect */
}

/* --- Text Fade-in Animation (Button Text Animation) --- */
@keyframes fadeInText {
    0% {
        opacity: 0;    /* Text starts as invisible */
        transform: translateY(-10px);  /* Text starts slightly above */
    }
    100% {
        opacity: 1;    /* Text becomes fully visible */
        transform: translateY(0);  /* Text settles into place */
    }
}
</string>
